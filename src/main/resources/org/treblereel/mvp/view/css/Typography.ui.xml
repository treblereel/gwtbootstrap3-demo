<ui:UiBinder xmlns:ui="urn:ui:org.gwtproject.uibinder"
             xmlns:g="urn:import:org.gwtproject.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
             xmlns:d="urn:import:org.treblereel.ui">

    <b:Row>
        <b:Column size="XS_12">

            <b:PageHeader text="Typography" subText="Built in text support"/>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Headings"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b:Heading size="H6" subText="Test"/>
                    <b:Heading size="H1" text="Test"/>
                    <b:Heading size="H1" text="h1. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H2" text="h2. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H3" text="h3. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H4" text="h4. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H5" text="h5. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H6" text="h6. Bootstrap heading" subText="subtext"/>
                    <b:Heading size="H6" subText="subtext" text="Test"/>
                    <b:Heading size="H1">
                        <b:Icon type="GEAR"/>
                        <b.html:Text text="h1. Heading with Icon"/>
                    </b:Heading>
                    <b:Heading size="H1">
                        <b.html:Text text="h1. Heading with Icon"/>
                        <b.html:Small text=" Sub"/>
                    </b:Heading>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:Heading size="H1" subText="..."/&gt;\n
                        &lt;b:Heading size="H2" subText="..."/&gt;\n
                        &lt;b:Heading size="H3" subText="..."/&gt;\n
                        &lt;b:Heading size="H4" subText="..."/&gt;\n
                        &lt;b:Heading size="H5" subText="..."/&gt;\n
                        &lt;b:Heading size="H6" subText="..."/&gt;\n\n
                        &lt;b:Heading size="H1"&gt;\n
                        \s\s&lt;b:Icon type="..."/&gt;\n
                        \s\s&lt;b.html:Text type="..."/&gt;\n
                        &lt;/b:Heading&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Body Copy"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b:Lead text="Make a paragraph stand out!"/>
                    <b.html:Small text="This line of text is meant to be treated as fine print."/>
                    <b.html:ClearFix/>
                    <b.html:Strong text="The following snippet of text is rendered as bold text."/>
                    <b.html:ClearFix/>
                    <b.html:Italic text="The following snippet of text is rendered as italicized text."/>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:Lead text=""/&gt;\n
                        &lt;b.html:Small text=""/&gt;\n
                        &lt;b.html:Strong text=""/&gt;\n
                        &lt;b.html:Italic text=""/&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Text Emphasis"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b.html:Paragraph emphasis="MUTED">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph emphasis="PRIMARY">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph emphasis="SUCCESS">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph emphasis="INFO">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph emphasis="WARNING">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph emphasis="DANGER">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:Paragraph emphasis="MUTED"/&gt;\n
                        &lt;b.html:Paragraph emphasis="PRIMARY"/&gt;\n
                        &lt;b.html:Paragraph emphasis="SUCCESS"/&gt;\n
                        &lt;b.html:Paragraph emphasis="INFO"/&gt;\n
                        &lt;b.html:Paragraph emphasis="WARNING"/&gt;\n
                        &lt;b.html:Paragraph emphasis="DANGER"/&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Contextual Backgrounds"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b.html:Paragraph contextualBackground="DEFAULT">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph contextualBackground="PRIMARY">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph contextualBackground="SUCCESS">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph contextualBackground="INFO">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph contextualBackground="WARNING">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                    <b.html:Paragraph contextualBackground="DANGER">Fusce dapibus, tellus ac cursus commodo</b.html:Paragraph>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:Paragraph contextualBackground="DEFAULT"/&gt;\n
                        &lt;b.html:Paragraph contextualBackground="PRIMARY"/&gt;\n
                        &lt;b.html:Paragraph contextualBackground="SUCCESS"/&gt;\n
                        &lt;b.html:Paragraph contextualBackground="INFO"/&gt;\n
                        &lt;b.html:Paragraph contextualBackground="WARNING"/&gt;\n
                        &lt;b.html:Paragraph contextualBackground="DANGER"/&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Text Alignment"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b.html:Paragraph alignment="LEFT">Align Left</b.html:Paragraph>
                    <b.html:Paragraph alignment="CENTER">Align Center</b.html:Paragraph>
                    <b.html:Paragraph alignment="RIGHT">Align Right</b.html:Paragraph>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:Paragraph alignment="LEFT"/&gt;\n
                        &lt;b.html:Paragraph alignment="CENTER"/&gt;\n
                        &lt;b.html:Paragraph alignment="RIGHT"/&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Abbreviations"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <g:HTMLPanel>
                        An abbreviation of the word attribute is
                        <b:Abbreviation title="attribute">attr.</b:Abbreviation>
                    </g:HTMLPanel>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:Abbreviation title=""/&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Block Quotes"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b:BlockQuote>
                        <b.html:Paragraph>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere
                            erat a ante.
                        </b.html:Paragraph>
                    </b:BlockQuote>
                    <b.html:ClearFix/>
                    <b:BlockQuote>
                        <b.html:Paragraph>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere
                            erat a ante.
                        </b.html:Paragraph>
                        <b.html:Small>Someone Famous</b.html:Small>
                    </b:BlockQuote>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:BlockQuote&gt;\n
                        \s\s&lt;b.html:Paragraph/&gt;\n
                        &lt;/b:BlockQuote&gt;\n
                    </d:PrettyPre>
                    <b.html:ClearFix/>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:BlockQuote&gt;\n
                        \s\s&lt;b.html:Paragraph/&gt;\n
                        \s\s&lt;b.html:Small/&gt;\n
                        &lt;/b:BlockQuote&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Lists"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b.html:OrderedList>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                    </b.html:OrderedList>

                    <b.html:UnorderedList>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                    </b.html:UnorderedList>

                    <b.html:UnorderedList inline="true">
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                        <b:AnchorListItem text="Item"/>
                    </b.html:UnorderedList>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:OrderedList&gt;\n
                        \s\s&lt;b:AnchorListItem/&gt;\n
                        &lt;/b.html:OrderedList&gt;\n
                    </d:PrettyPre>
                    <b.html:ClearFix/>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:UnorderedList&gt;\n
                        \s\s&lt;b:AnchorListItem/&gt;\n
                        &lt;/b.html:UnorderedList&gt;\n
                    </d:PrettyPre>
                    <b.html:ClearFix/>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b.html:UnorderedList inline="true"&gt;\n
                        \s\s&lt;b:AnchorListItem/&gt;\n
                        &lt;/b.html:UnorderedList&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>

            <b:Panel>
                <b:PanelHeader>
                    <b:Heading size="H3" text="Description"/>
                </b:PanelHeader>
                <b:PanelBody>
                    <b:Description>
                        <b:DescriptionTitle text="Euismod"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                        <b:DescriptionTitle text="Malesuada porta"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                        <b:DescriptionTitle text="Semper"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                    </b:Description>

                    <b:Description horizontal="true">
                        <b:DescriptionTitle text="Euismod"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                        <b:DescriptionTitle text="Malesuada porta"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                        <b:DescriptionTitle text="Semper"/>
                        <b:DescriptionData text="Etiam porta sem malesuada magna mollis euismod."/>
                    </b:Description>
                </b:PanelBody>
                <b:PanelFooter>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:Description&gt;\n
                        \s\s&lt;b:DescriptionTitle text=""/&gt;\n
                        \s\s&lt;b:DescriptionData text=""/&gt;\n
                        &lt;/b:Description&gt;\n
                    </d:PrettyPre>
                    <b.html:ClearFix/>
                    <d:PrettyPre addStyleNames="lang-xml">
                        &lt;b:Description horizontal="true"&gt;\n
                        \s\s&lt;b:DescriptionTitle text=""/&gt;\n
                        \s\s&lt;b:DescriptionData text=""/&gt;\n
                        &lt;/b:Description&gt;\n
                    </d:PrettyPre>
                </b:PanelFooter>
            </b:Panel>
        </b:Column>

    </b:Row>

</ui:UiBinder>
